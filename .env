# Server Configuration
NODE_ENV=development
PORT=3000
CLIENT_URL=http://localhost:5173

# Supabase Configuration (MOCK - no se usan realmente)
VITE_SUPABASE_URL=https://mock.supabase.co
VITE_SUPABASE_ANON_KEY=mock_anon_key_12345
SUPABASE_SERVICE_ROLE_KEY=mock_service_role_key

# Database (No se usa en modo mock)
DATABASE_URL=postgresql://mock:mock@localhost:5432/mock

# JWT Configuration (No se usa en modo mock)
JWT_SECRET=mock_jwt_secret_key
JWT_EXPIRES_IN=7d
WS_JWT_SECRET=mock_ws_jwt_secret

# Claude AI Configuration (No se usa por ahora)
ANTHROPIC_API_KEY=mock_api_key
CLAUDE_MODEL=claude-sonnet-4-20250514
CLAUDE_MAX_TOKENS=8000

# AI Feature Flags
ENABLE_AI_ANALYSIS=false
ENABLE_AI_DIAGNOSIS=false
ENABLE_AI_REPORTS=false
AI_RATE_LIMIT_PER_HOUR=100

# WebSocket Configuration
WS_PORT=3000
WS_PATH=/ws
WS_PING_INTERVAL=30000
WS_RECONNECT_ATTEMPTS=5

# Security
CORS_ORIGIN=http://localhost:5173
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true

# Feature Flags
ENABLE_CHAT=true
ENABLE_APPOINTMENTS=true
ENABLE_TESTIMONIALS=true